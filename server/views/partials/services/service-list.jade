.container.top-padding-med
  h1 ""
  h1 Services
  table.table.table-hover.table-striped.table-condensed
    thead
      tr
        th Service
        th Cost
        th Description
      tbody
        tr(ng-repeat="service in services")
          td {{service.name}}
          td {{service.cost |  currency}}
          td {{service.description}}
