h1 ""
.container
  table.table.table-hover.table-striped.table-condensed
    thead
      tr 
        th Service Name
        th Featured
        th Cost
        th Description
        th Reviews
    tbody
      tr(ng-repeat="service in services")
        td {{service.name}}
        td {{service.featured}}
        td {{service.cost}}
        td {{service.description}}
        td {{service.reviews}}
