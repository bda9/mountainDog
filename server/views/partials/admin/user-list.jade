h1 ""
.container
  table.table.table-hover.table-striped.table-condensed
    thead
      tr 
        th First Name
        th Last Name
        th Phone Number
        th Dog Name
        th Dog Breed
    tbody
      tr(ng-repeat="user in users")
        td {{user.firstName}}
        td {{user.lastName}}
        td {{user.phone}}
        td {{user.dogName}}
        td {{user.breed}}
